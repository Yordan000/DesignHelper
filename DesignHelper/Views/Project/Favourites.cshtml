@model IEnumerable<ProjectServiceModel>

@{
    ViewBag.Title = "Favourites";
}


<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
        <div class="row">
        @foreach (var project in Model)
        {
          <partial name="_ProjectPartial" model="@project" />
        }
        </div>
}
else
{
        <h3 class="text-center">You have no projects yet!</h3>
}